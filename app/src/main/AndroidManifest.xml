<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RushApp"
        tools:targetApi="31">
        <activity
            android:name=".MechanicsActivity"
            android:exported="false"
            android:label="@string/title_activity_mechanics"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".VehicleActivity"
            android:exported="false"
            android:label="@string/title_activity_vehicle"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".InvoicesActivity"
            android:exported="false"
            android:label="@string/title_activity_invoices"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".BookingsActivity"
            android:exported="false"
            android:label="@string/title_activity_bookings"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".EditUserActivity"
            android:exported="false"
            android:label="@string/title_activity_edit_user"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".NewUserActivity"
            android:exported="false"
            android:label="@string/title_activity_create_account"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".CreateAccountActivity"
            android:exported="false"
            android:label="@string/title_activity_create_account"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.RushApp" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.RushApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>